{
	"name": "inkeep-agents-quickstart",
	"version": "0.1.0",
	"description": "An Inkeep Agent Framework directory",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "concurrently \"turbo dev\" \"inkeep dev\" \"sleep 5 && curl -s http://localhost:2001 > /dev/null && echo '✅ Inkeep UI ready at http://localhost:2001' || echo '⚠️  Inkeep UI starting on http://localhost:2001'\" --names \"APIs,UI,Status\" --prefix-colors \"blue,green,yellow\" --success first",
		"dev:apis": "turbo dev",
		"dev:ui": "inkeep dev",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"postinstall": "node scripts/postinstall.js"
	},
	"dependencies": {
		"@inkeep/agents-core": "^0.14.2",
		"@inkeep/agents-run-api": "0.14.1",
		"@inkeep/agents-sdk": "^0.14.2",
		"dotenv": "^16.0.0",
		"zod": "^4.1.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.0",
		"@inkeep/agents-cli": "^0.14.2",
		"concurrently": "^8.2.0",
		"drizzle-kit": "^0.31.4",
		"tsx": "^4.19.0",
		"turbo": "^2.5.5",
		"wait-on": "^8.0.0"
	},
	"engines": {
		"node": ">=22.x"
	},
	"packageManager": "pnpm@10.10.0",
	"pnpm": {
		"onlyBuiltDependencies": ["keytar"]
	}
}
